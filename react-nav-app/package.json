{
  "name": "react-nav-app",
  "version": "0.1.0",
  "description": "A simple server stencil based on node and used jest for test",
  "main": "lib/index.js",
  "engines": {
    "node": ">= 6"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "clean": "rm -rf ./dist",
    "build:pack": "webpack --config config/webpack.config.js",
    "build:pack:watch": "watch 'yarn build:pack' ./src",
    "build": "yarn clean && yarn build:pack",
    "lint": "eslint src",
    "test": "jest --coverage --config config/jest.config.js",
    "test:watch": "watch 'yarn test' ./src",
    "start": "node server.js",
    "start:dev": "webpack-dev-server --config config/webpack.config.dev.js --open"
  },
  "dependencies": {
    "@mixspa/app": "^0.4.1",
    "classnames": "^2.2.6",
    "express": "^4.16.4",
    "morgan": "^1.9.1",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.4.6",
    "babel-core": "^7.0.0-bridge",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "eslint": "^5.12.1",
    "eslint-plugin-jest": "^22.1.3",
    "eslint-plugin-react": "^7.12.4",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.5.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.10.0",
    "postcss-loader": "^3.0.0",
    "react-test-renderer": "^16.7.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "watch": "^1.0.2",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xqstencils/node-jest-react-server.git"
  },
  "keywords": [
    "node",
    "babel",
    "jest",
    "react",
    "server",
    "eslint",
    "security",
    "circleci"
  ],
  "author": "Cao Xiaoqi<xqcao@xqcao.com>",
  "contributors": [
    "Xiaoqi Cao"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xqstencils/node-jest-react-server/issues"
  },
  "homepage": "https://github.com/xqstencils/node-jest-react-server#readme"
}
